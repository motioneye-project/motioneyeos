+#ifndef _ASM_NIOS_BITOPS_H_
+#define _ASM_NIOS_BITOPS_H_
+
+/*--------------------------------------------------------------------
+ *
+ * include/asm-nios2nommu/bitops.h
+ *
+ * Derived from various works, Alpha, ix86, M68K, Sparc, ...et al
+ *
